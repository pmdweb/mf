{
	"root": true,
	"env": {
	  "browser": true,
	  "node": true
	},
	"extends": ["next/core-web-vitals"],
	"parserOptions": {
	  "ecmaFeatures": {
		"jsx": true
	  },
	  "ecmaVersion": 2021,
	  "sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-tabs": "off",
		"indent": ["error", "tab"]
	  },
	"settings": {
	  "react": {
		"version": "detect"
	  }
	}
  }